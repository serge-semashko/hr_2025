<!DOCTYPE fontscolors PUBLIC
    "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN"
    "https://netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor name="character" default="char"/>
    <fontcolor name="errors" default="error"/>
    <fontcolor name="identifier" default="identifier" foreColor="black" >
    </fontcolor>
        
        
    <fontcolor name="keyword" default="keyword" foreColor="red"/>
    <fontcolor name="literal" default="keyword" />
    <fontcolor name="comment" default="comment"/>
    <fontcolor name="number" default="number"/>
    <fontcolor name="operator" default="operator"/>
    <fontcolor name="string" default="string"/>
    <fontcolor name="separator" default="separator"/>
    <fontcolor name="whitespace" default="whitespace"/>
        
    <fontcolor name="label"  default="keyword" foreColor="gray" />
    <fontcolor name="go" default="keyword" foreColor="red" />
    <fontcolor name="machine" default="keyword" foreColor="red" />       
    <fontcolor name="tool" default="keyword" foreColor="00CD00" >
        <font style="bold" />
    </fontcolor>
    <fontcolor name="speed" default="keyword" foreColor="blue"/>
    <fontcolor name="axis" default="keyword" foreColor="black" >
        <font style="bold" />
    </fontcolor>
    <fontcolor name="lpara" default="keyword" foreColor="EE7600" >
        <font style="bold" />
    </fontcolor>
    <fontcolor name="sect" default="keyword" foreColor="00FFFF" bgColor="black">
        <font style="bold" />
    </fontcolor>
    <fontcolor name="prognr" default="keyword" foreColor="black" bgColor="gray" >
        <font style="bold" />
    </fontcolor>


</fontscolors>


 

